ntDefault();\n\n    const {\n      user: { username },\n      topic,\n      title,\n      body\n    } = this.state;\n\n    try {\n      const newArticle = await postArticle(username, topic, title, body);\n\n      this.setState({\n        user: this.context.user,\n        topic: '',\n        title: '',\n        body: '',\n        isPosted: true,\n        newArticleId: newArticle.article_id,\n        hasError: false,\n        errMsg: ''\n      });\n    } catch (err) {\n      const {\n        response: { status, statusText }\n      } = err;\n\n      this.setState({\n        isLoading: false,\n        hasError: true,\n        errMsg: `${status}: ${statusText}`\n      });\n    }\n  };\n\n  render() {\n    const {\n      user: { name },\n      isPosted,\n      newArticleId\n    } = this.state;\n\n    return (\n      <main>\n        {!isPosted ? (\n          <>\n            <h2 className=\"articles__header\">What's on your mind, {name}?</h2>\n            <div className=\"articles\">\n              <form className=\"new-article\" onSubmit={this.handleSubmit}>\n                <input\n                  type=\"text\"\n                  name=\"topic\"\n                  placeholder=\"Topic\"\n                  required\n                  className=\"new-article__input\"\n                  onChange={this.handleChange}\n                ></input>\n                <input\n                  type=\"text\"\n                  name=\"title\"\n                  placeholder=\"Title\"\n                  required\n                  className=\"new-article__input\"\n                  onChange={this.handleChange}\n                ></input>\n\n                <textarea\n                  name=\"body\"\n                  value={this.state.comment}\n                  placeholder=\"Impart your wisdom here\"\n                  required\n                  className=\"new-article__input__body\"\n                  onChange={this.handleChange}\n                ></textarea>\n                <input type=\"submit\" className=\"new-article__submit\"></input>\n              </form>\n            </div>\n          </>\n        ) : (\n          <>\n            <h2 className=\"articles__header\">Article posted!</h2>\n            <div className=\"articles\">\n              <h3 className=\"new-article__success\">Want to look?</h3>\n              <Link to={`/article/${newArticleId}`}>\n                <button className=\"new-article__success-btn\">\n                  Heck, yeah!\n                </button>\n              </Link>\n            </div>\n          </>\n        )}\n      </main>\n    );\n  }\n}\n\nNewArticle.contextType = UserContext;\n\nexport default NewArticle;\n\n/*\n    title: 'Kindles: tech magic or work of the dark one?',\n    body:\n      'Book-lovers of the world unite to condemn this newfangled devilry. Tech-fans strike back.',\n    topic: 'paper',\n    author: 'rogersop',\n  })\n  .expect(201)\n  .then(({ body }) => {\n    expect(body.article).toEqual({\n      article_id: 13,\n      title: 'Kindles: tech magic or work of the dark one?',\n      body:\n        'Book-lovers of the world unite to condemn this newfangled devilry. Tech-fans strike back.',\n      votes: 0,\n      topic: 'paper',\n      author: 'rogersop',\n      created_at: expect.any(String),\n\n*/\n","/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/ArticleList/ArticleList.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/CommentList/CommentList.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Nav/Nav.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Modal.jsx",[],{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":4,"column":8,"nodeType":"109","messageId":"110","endLine":4,"endColumn":17},"no-native-reassign",["111"],"no-negated-in-lhs",["112"],"no-unused-vars","'SidePanel' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]