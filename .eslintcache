assName=\"sidepanel__box\">\n        {page === 'newArticle' ? (\n          <NewPostRules />\n        ) : (\n          <>\n            <TopicBox topicSlug={topic} />\n            <TopArticles topicSlug={topic} />\n          </>\n        )}\n      </div>\n      {user && page !== 'newArticle' && page !== 'article' && (\n        <Link\n          to={topic ? `/article/new/${topic}` : '/article/new'}\n          className=\"sidepanel__link\"\n        >\n          <button className=\"mainButton\">\n            <span className=\"mainButton__text\">Post new article</span>\n          </button>\n        </Link>\n      )}\n      {page !== 'articleList' && (\n        <Link\n          to={topic ? `/articles/${topic}` : '/'}\n          className=\"sidepanel__link\"\n        >\n          <button className=\"mainButton\">\n            <span className=\"mainButton__text\">Back to articles</span>\n          </button>\n        </Link>\n      )}\n    </div>\n  );\n};\n\nexport default SidePanel;\n","/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/SidePanel/TopicBox.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/CommentList/CommentForm.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Votes.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/SidePanel/TopArticles.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Contexts/UserContext.js",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/NewArticle.jsx",["117"],"import React, { Component } from 'react';\n\nimport { UserContext } from '../Contexts/UserContext';\nimport { postArticle } from '../Utils/api';\nimport { formatTitle } from '../Utils/utils';\n\nimport SidePanel from './SidePanel';\n\nclass NewArticle extends Component {\n  state = {\n    user: this.context.user,\n    topic: '',\n    title: '',\n    body: '',\n    isPosted: false,\n    newArticleId: null,\n    hasError: false,\n    errMsg: '',\n  };\n\n  componentDidMount = () => {\n    const { topic } = this.props;\n\n    if (topic) {\n      this.setState({ topic: formatTitle(topic) });\n    }\n  };\n\n  handleChange = (event) => {\n    this.setState({ [event.target.name]: event.target.value });\n  };\n\n  handleSubmit = async (event) => {\n    event.preventDefault();\n\n    const {\n      user: { username },\n      topic,\n      title,\n      body,\n    } = this.state;\n\n    const { showNewModal } = this.props;\n\n    try {\n      const newArticle = await postArticle(username, topic, title, body);\n      this.setState(\n        {\n          user: this.context.user,\n          topic: '',\n          title: '',\n          body: '',\n          isPosted: true,\n          newArticleId: newArticle.article_id,\n          hasError: false,\n          errMsg: '',\n        },\n        () => {\n          const { newArticleId } = this.state;\n          showNewModal(\n            true,\n            'Article Created',\n            'Take a look!',\n            `/article/${newArticleId}`\n          );\n        }\n      );\n    } catch (err) {\n      const {\n        response: { status, statusText },\n      } = err;\n\n      this.setState({\n        isLoading: false,\n        hasError: true,\n        errMsg: `${status}: ${statusText}`,\n      });\n    }\n  };\n\n  render() {\n    const {\n      user: { name },\n      isPosted,\n      newArticleId,\n    } = this.state;\n\n    const { topic } = this.props;\n\n    return (\n      <main>\n        {!isPosted ? (\n          <>\n            <h2 className=\"articles__header\">What's on your mind, {name}?</h2>\n            <div className=\"articles\">\n              <form className=\"new-article\" onSubmit={this.handleSubmit}>\n                <input\n                  type=\"text\"\n                  name=\"topic\"\n                  placeholder=\"Topic\"\n                  value={this.state.topic}\n                  required\n                  className=\"new-article__input\"\n                  onChange={this.handleChange}\n                ></input>\n                <input\n                  type=\"text\"\n                  name=\"title\"\n                  placeholder=\"Title\"\n                  required\n                  className=\"new-article__input\"\n                  onChange={this.handleChange}\n                ></input>\n\n                <textarea\n                  name=\"body\"\n                  value={this.state.comment}\n                  placeholder=\"Impart your wisdom here\"\n                  required\n                  className=\"new-article__textArea\"\n                  onChange={this.handleChange}\n                ></textarea>\n                <input\n                  type=\"submit\"\n                  value=\"post\"\n                  className=\"mainButton new-article__submitBtn\"\n                ></input>\n              </form>\n            </div>\n            <SidePanel topic={topic} page=\"newArticle\" />\n          </>\n        ) : null}\n      </main>\n    );\n  }\n}\n\nNewArticle.contextType = UserContext;\n\nexport default NewArticle;\n","/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/ArticleList/ArticleList.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/CommentList/CommentList.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Nav/Nav.jsx",["118"],"import React, { Component } from 'react';\nimport TopicList from './TopicList';\nimport User from '../User/User';\nimport { UserContext } from '../../Contexts/UserContext';\nimport TopicNav from './TopicNav';\n\nclass Nav extends Component {\n  state = {\n    showTopics: false,\n  };\n\n  handleTopicBtn = () => {\n    this.setState((currState) => {\n      return { showTopics: !currState.showTopics };\n    });\n  };\n\n  render() {\n    const { showTopics } = this.state;\n    const { user } = this.context;\n\n    return (\n      <nav className=\"nav grid\">\n        <div className=\"nav__bar grid\">\n          <div className=\"nav__bar__controls\">\n            <div className=\"topics__container\">\n              <TopicNav\n                showTopics={this.state.showTopics}\n                onClick={this.handleTopicBtn}\n              >\n                Topics\n              </TopicNav>\n            </div>\n            <User />\n          </div>\n        </div>\n\n        {showTopics ? <TopicList /> : null}\n      </nav>\n    );\n  }\n}\n\nNav.contextType = UserContext;\n\nexport default Nav;\n","/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Modal.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Utils/api.js",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Utils/utils.js",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/User/User.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/User/LoginModal.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Nav/TopicNav.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/SidePanel/NewPostRules.jsx",[],{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":1,"column":17,"nodeType":"125","messageId":"126","endLine":1,"endColumn":25},{"ruleId":"123","severity":1,"message":"127","line":85,"column":7,"nodeType":"125","messageId":"126","endLine":85,"endColumn":19},{"ruleId":"123","severity":1,"message":"128","line":20,"column":13,"nodeType":"125","messageId":"126","endLine":20,"endColumn":17},"no-native-reassign",["129"],"no-negated-in-lhs",["130"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'newArticleId' is assigned a value but never used.","'user' is assigned a value but never used