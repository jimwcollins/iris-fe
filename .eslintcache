../Utils/api';\nimport SidePanel from './SidePanel';\nimport { Link } from '@reach/router';\n\nclass NewArticle extends Component {\n  state = {\n    user: this.context.user,\n    topic: '',\n    title: '',\n    body: '',\n    isPosted: false,\n    newArticleId: null,\n    hasError: false,\n    errMsg: ''\n  };\n\n  handleChange = (event) => {\n    this.setState({ [event.target.name]: event.target.value });\n  };\n\n  handleSubmit = async (event) => {\n    event.preventDefault();\n\n    const {\n      user: { username },\n      topic,\n      title,\n      body\n    } = this.state;\n\n    const { showNewModal } = this.props;\n\n    try {\n      const newArticle = await postArticle(username, topic, title, body);\n      this.setState(\n        {\n          user: this.context.user,\n          topic: '',\n          title: '',\n          body: '',\n          isPosted: true,\n          newArticleId: newArticle.article_id,\n          hasError: false,\n          errMsg: ''\n        },\n        () => {\n          const { newArticleId } = this.state;\n          showNewModal(\n            true,\n            'Article Created',\n            'Take a look!',\n            `/article/${newArticleId}`\n          );\n        }\n      );\n    } catch (err) {\n      const {\n        response: { status, statusText }\n      } = err;\n\n      this.setState({\n        isLoading: false,\n        hasError: true,\n        errMsg: `${status}: ${statusText}`\n      });\n    }\n  };\n\n  render() {\n    const {\n      user: { name },\n      isPosted,\n      newArticleId\n    } = this.state;\n\n    return (\n      <main>\n        {!isPosted ? (\n          <>\n            <h2 className=\"articles__header\">What's on your mind, {name}?</h2>\n            <div className=\"articles\">\n              <form className=\"new-article\" onSubmit={this.handleSubmit}>\n                <input\n                  type=\"text\"\n                  name=\"topic\"\n                  placeholder=\"Topic\"\n                  required\n                  className=\"new-article__input\"\n                  onChange={this.handleChange}\n                ></input>\n                <input\n                  type=\"text\"\n                  name=\"title\"\n                  placeholder=\"Title\"\n                  required\n                  className=\"new-article__input\"\n                  onChange={this.handleChange}\n                ></input>\n\n                <textarea\n                  name=\"body\"\n                  value={this.state.comment}\n                  placeholder=\"Impart your wisdom here\"\n                  required\n                  className=\"new-article__input__body\"\n                  onChange={this.handleChange}\n                ></textarea>\n                <input type=\"submit\" className=\"new-article__submit\"></input>\n              </form>\n            </div>\n          </>\n        ) : null}\n      </main>\n    );\n  }\n}\n\nNewArticle.contextType = UserContext;\n\nexport default NewArticle;\n","/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/ArticleList/ArticleList.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/CommentList/CommentList.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Nav/Nav.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Modal.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Utils/api.js",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Utils/utils.js",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/User/User.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/User/LoginModal.jsx",[],"/Users/james/Code/3. Learning/northcoders/frontend/Iris/iris-fe/src/Components/Nav/TopicNav.jsx",[],{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"11